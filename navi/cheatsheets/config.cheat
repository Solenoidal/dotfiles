% config

# zsh
nvim ~/.zshrc

# fish
nvim ~/.config/fish/config.fish

# starship
nvim ~/.config/starship/config.toml

# espanso for ubuntu
nvim ~/.config/espanso/default.yml
# espanso for mac
nvim ~/Library/Preferences/espanso/default.yml

# navi
cd ~/.dotfiles/navi && nvim $(fd --type file | fzf)
